{
  "name": "cypressrepo",
  "version": "1.0.0",
  "description": "cypress e2e testing",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy-allure": "npx cypress run --env allure=true --reporter mocha-allure-reporter",
